@model IEnumerable<IBayiLibrary.Models.Domain.tblOrder> 

@{
    ViewData["Title"] = "Load Prescription";
    Layout = "PharmacistLayout";
}
<h1>Prescription Details</h1>
<div>
    @foreach (var item in Model)
    {
        <dl class="row">
            <dt class="col-sm-2">@Html.DisplayNameFor(m => item.LastName)</dt> 
            <dd class="col-sm-10">@item.LastName</dd> 
            <dt class="col-sm-2">@Html.DisplayNameFor(m => item.FirstName)</dt>
            <dd class="col-sm-10">@item.FirstName</dd>
            <dt class="col-sm-2">@Html.DisplayNameFor(m => item.MedicineName)</dt> 
            <dd class="col-sm-10">@item.MedicineName</dd> 
            <dt class="col-sm-2">@Html.DisplayNameFor(m => item.SalesPrice)</dt>
            <dd class="col-sm-10">@item.SalesPrice</dd> 
            <dt class="col-sm-2">@Html.DisplayNameFor(m => item.Quantity)</dt>
            <dd class="col-sm-10">@item.Quantity</dd>
            <dt class="col-sm-2">@Html.DisplayNameFor(m => item.TotalSales)</dt> 
            <dd class="col-sm-10">@item.TotalSales</dd> 
        </dl>
        }
        <hr />
</div>
